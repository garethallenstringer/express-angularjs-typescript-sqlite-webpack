{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "object-literal-sort-keys": false
    },
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "trailing-comma": true,
        "object-literal-key-quotes": false,
        "prefer-for-of": true,
        "curly": ["ignore-same-line"],
        "no-namespace": false,
        "deprecation": {
            "severity": "warn"
        },
        "arrow-parens": true,
        "no-redundant-jsdoc": true,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
